{
  "name": "@monju/web",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "run-p 'dev:*'",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "build": "pathpida --enableStatic && prisma generate && next build",
    "start": "next start",
    "lint": "yarn g:lint",
    "format": "yarn g:format",
    "api:build": "../../node_modules/.bin/aspida",
    "api:regenerate": "rm -rf src/api && mkdir src/api && openapi2aspida",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aspida/axios": "^1.10.2",
    "@hookform/resolvers": "^2.8.8",
    "@prisma/client": "^4.0.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.4",
    "next": "^12.2.0",
    "next-auth": "^4.10.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.31.1",
    "react-icons": "^4.4.0",
    "recoil": "^0.6.1",
    "swr": "^1.2.2",
    "ts-pattern": "^4.0.5",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@monju/config": "link:../../packages/tsconfig",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.2",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.19.0",
    "pathpida": "^0.18.0",
    "postcss": "^8.4.8",
    "prisma": "^4.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "packageManager": "yarn@3.2.1"
}
